function max1020(a, b){
  if((a>=10 && a<=20) || (b>=10 && b<=20)){
    
      if(a>20 || b>20){
        let max = 20;
        let num

        if(a>max){
          num = b;
        } 
        if(b>max){
          num = a;
        }

        return num
      }
    
    if(a<=20 && b<=20){
      let max1 = a;
      if(b>max1){
        max1=b;
      }
      return max1
    }
    
    
    
    
       
  } else return 0
  
}